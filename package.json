{
  "name": "all-in-web",
  "version": "1.0.2",
  "evtTrackingVersion": "v1.0",
  "description": "all-in-web",
  "author": "sst-fe",
  "clientAppId": "all-in-web",
  "scripts": {
    "add": "node build/add-module.js",
    "del": "node build/del-module.js",
    "dev": "cross-env NODE_ENV=development node build/development.js",
    "lint": "cross-env NODE_ENV=production eslint --fix --ext .js,.vue src && cross-env FORCE_COLOR=1 node ./build/verfiy-code-style.js",
    "build:portal": "cross-env NODE_ENV=production parallel-webpack --config build/webpack.prod.conf.js --no-stats",
    "build:pub": "cross-env NODE_ENV=publish parallel-webpack --config build/webpack.pub.conf.js --no-stats",
    "build": "npm-run-all build:* --parallel",
    "dev:alarm": "npm run dev --nocache --module=alarm-history,surveillance-task,results,alarm-realtime,cascade-alarm-history,cascade-surveillance-task",
    "megvii-ci:dockerbuild": "dockerbuild",
    "megvii-ci:dockerpush": "dockerpush"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && cross-env FORCE_COLOR=1 node ./build/verfiy-code-style.js",
      "pre-push": "lint-staged",
      "commit-msg": "node ./build/verify-commit-msg.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "cross-env NODE_ENV=production eslint --max-warnings 0"
  },
  "dependencies": {
    "@megvii-gis/calc-api": "^2.2.0",
    "@megvii-gis/core-api": "^2.2.0",
    "@megvii-gis/core-class": "^2.2.0",
    "@megvii-gis/style-sets": "0.0.7",
    "@megvii-gis/vue-components": "^2.2.2",
    "@megvii-i18n/vue-i18n": "^1.0.0-alpha.20",
    "@megvii-video-player/plugin-structure-drawer": "^2.0.26",
    "@megvii-video-player/plugin-video-build-in": "^2.0.18",
    "@megvii-video-player/vue-player": "^2.0.18",
    "@vue/composition-api": "^1.0.0-beta.14",
    "countup.js": "^2.0.5",
    "dom-to-image": "^2.6.0",
    "echarts": "^4.7.0",
    "megvii-http": "^1.0.2",
    "megvii-roi": "^1.7.0",
    "megvii-ui-pro": "^1.1.8",
    "megvii-websocket": "^2.0.3",
    "vue": "~2.6.10",
    "vue-router": "~3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@megvii-i18n/cli": "^1.0.0-alpha.17",
    "@vue/babel-preset-jsx": "^1.0.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "chalk": "^2.4.2",
    "consola": "^2.10.1",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^3.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "esbuild-minimizer-webpack-plugin": "^1.0.0-beta.2",
    "eslint": "^6.7.2",
    "eslint-config-megvii": "^1.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.2",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "json-templater": "^1.2.0",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.15",
    "megvii-ci-utils": "^1.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "~2.24.0",
    "node-notifier": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "parallel-webpack": "^2.6.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^7.3.2",
    "rimraf": "^2.6.2",
    "sass": "^1.26.5",
    "sass-loader": "^9.0.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "thread-loader": "^2.1.3",
    "url-loader": "^0.6.2",
    "vue-loader": "~15.4.0",
    "vue-template-compiler": "~2.6.10",
    "vuex": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-build-notifier": "^1.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "webpack-virtual-modules": "^0.2.2",
    "webpackbar": "^3.2.0"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "chrome >= 80"
  ]
}
